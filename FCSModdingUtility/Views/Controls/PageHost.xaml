<UserControl x:Class="FCSModdingUtility.PageHost"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FCSModdingUtility"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" x:ClassModifier="public">
    <Grid>
        <ContentControl x:Name="OldPage"/>
        <ContentControl x:Name="NewPage"/>
    </Grid>
</UserControl>
